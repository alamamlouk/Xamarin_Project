<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xamarin_Project.Views.Signup">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Sign up"/>
            <Label Text="Name"/>
            <Entry x:Name="Name_entry"/>
            <Label Text="Username"/>
            <Entry x:Name="Username_entry"/>
            <Label Text="Password"/>
            <Entry x:Name="Password_entry"/>
            <Label Text="Confirm password"/>
            <Entry x:Name="Confirm_btn"/>
            <Button Text="SignUp" Clicked="Signup_User"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>